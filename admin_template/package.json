{
  "name": "admin_template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.5",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "npm run watch:sass | react-scripts start",
    "watch:sass": "npm run build:sass && node-sass-chokidar ./src/styles/ -o ./src/styles/ --watch --recursive --include-path",
    "build": "react-scripts build",
    "build:sass": "node-sass-chokidar ./src/styles/ -o ./src/styles/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
